@article{djorgovskiCatalinaRealTimeTransient2011,
  title = {The {{Catalina Real-Time Transient Survey}} ({{CRTS}})},
  author = {Djorgovski, S. G. and Drake, A. J. and Mahabal, A. A. and Graham, M. J. and Donalek, C. and Williams, R. and Beshore, E. C. and Larson, S. M. and Prieto, J. and Catelan, M. and Christensen, E. and McNaught, R. H.},
  year = {2011},
  month = feb,
  journal = {arXiv:1102.5004 [astro-ph]},
  eprint = {1102.5004},
  eprinttype = {arxiv},
  primaryclass = {astro-ph},
  abstract = {Catalina Real-Time Transient Survey (CRTS) is a synoptic sky survey uses data streams from 3 wide-field telescopes in Arizona and Australia, covering the total area of \textasciitilde 30,000 deg2, down to the limiting magnitudes \textasciitilde{} 20 - 21 mag per exposure, with time baselines from 10 min to 6 years (and growing); there are now typically \textasciitilde{} 200 - 300 exposures per pointing, and coadded images reach deeper than 23 mag. The basic goal of CRTS is a systematic exploration and characterization of the faint, variable sky. The survey has detected \textasciitilde{} 3,000 high-amplitude transients to date, including \textasciitilde{} 1,000 supernovae, hundreds of CVs (the majority of them previously uncatalogued), and hundreds of blazars / OVV AGN, highly variable and flare stars, etc. CRTS has a complete open data philosophy: all transients are published immediately electronically, with no proprietary period at all, and all of the data (images, light curves) will be publicly available in the near future, thus benefiting the entire astronomical community. CRTS is a scientific and technological testbed and precursor for the grander synoptic sky surveys to come.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics,Astrophysics - High Energy Astrophysical Phenomena,Astrophysics - Instrumentation and Methods for Astrophysics},
  file = {C\:\\Users\\sidch\\Zotero\\storage\\9WXITMUY\\Djorgovski et al. - 2011 - The Catalina Real-Time Transient Survey (CRTS).pdf;C\:\\Users\\sidch\\Zotero\\storage\\D44BLX95\\1102.html}
}

@article{duKinematicsRRLyrae2020,
  title = {Kinematics of {{RR Lyrae}} Stars in the {{Galactic}} Bulge with {{OGLE-IV}} and {{Gaia DR2}}},
  author = {Du, Hangci and Mao, Shude and Athanassoula, E and Shen, Juntai and Pietrukowicz, Pawel},
  year = {2020},
  month = oct,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {498},
  number = {4},
  pages = {5629--5642},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/staa2601},
  abstract = {ABSTRACT             We analyse the kinematics and spatial distribution of 15\,599 fundamental-mode RR Lyrae (RRL) stars in the Milky Way bulge by combining OGLE-IV photometric data and Gaia DR2 proper motions. We show that the longitudinal proper motions and the line-of-sight velocities can give similar results for the rotation in the Galactic central regions. The angular velocity of bulge RRLs is found to be around 35\,km\,s-1\,kpc-1, significantly smaller than that for the majority of bulge stars (50\textendash 60\,km\,s-1\,kpc-1); bulge RRLs have larger velocity dispersion (120\textendash 140\,km\,s-1) than younger stars. The dependence of the kinematics of the bulge RRLs on their metallicities is shown by their rotation curves and spatial distributions. Metal-poor RRLs ([Fe/H]\&lt;-1) show a smaller bar angle than metal-rich ones. We also find clues suggesting that RRLs in the bulge are not dominated by halo stars. These results might explain some previous conflicting results over bulge RRLs and help understand the chemodynamical evolution of the Galactic bulge.},
  langid = {english},
  file = {C\:\\Users\\sidch\\Zotero\\storage\\DTC7FAG5\\Du et al. - 2020 - Kinematics of RR Lyrae stars in the Galactic bulge.pdf}
}

